w>\n                                    <Col>Total:</Col>\n                                    <Col>${cart.totalPrice}</Col>\n                                </Row>\n                            </ListGroup.Item>\n\n\n                            <ListGroup.Item>\n                                {error && <Message variant='danger'>{error}</Message>}\n                            </ListGroup.Item>\n\n                            <ListGroup.Item>\n                                <Button\n                                    type='button'\n                                    className='btn-block'\n                                    disabled={cart.cartItems === 0}\n                                    onClick={placeOrder}\n                                >\n                                    Place Order\n                                </Button>\n                            </ListGroup.Item>\n\n                        </ListGroup>\n                    </Card>\n                </Col>\n            </Row>\n        </div>\n    )\n}\n\nexport default PlaceOrderScreen\n\n\n","/Users/richardkruse/Desktop/CodeLab/UdemyIvy/backend/frontend/src/screens/ProductListScreen.js",[],"/Users/richardkruse/Desktop/CodeLab/UdemyIvy/backend/frontend/src/screens/OrderListScreen.js",[],"/Users/richardkruse/Desktop/CodeLab/UdemyIvy/backend/frontend/src/screens/PaymentScreen.js",[],"/Users/richardkruse/Desktop/CodeLab/UdemyIvy/backend/frontend/src/screens/UserListScreen.js",[],"/Users/richardkruse/Desktop/CodeLab/UdemyIvy/backend/frontend/src/screens/ProfileScreen.js",[],"/Users/richardkruse/Desktop/CodeLab/UdemyIvy/backend/frontend/src/screens/OrderScreen.js",[],"/Users/richardkruse/Desktop/CodeLab/UdemyIvy/backend/frontend/src/screens/ShippingScreen.js",[],"/Users/richardkruse/Desktop/CodeLab/UdemyIvy/backend/frontend/src/components/SearchBox.js",[],"/Users/richardkruse/Desktop/CodeLab/UdemyIvy/backend/frontend/src/components/Message.js",[],"/Users/richardkruse/Desktop/CodeLab/UdemyIvy/backend/fr